<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="DropDownMenu">
        //Menu的字体颜色
        <attr name="menuTitleTextColor" format="color|reference" />
        //Menu的字体大小
        <attr name="menuTitleTextSize" format="dimension" />
        //Menu的背景
        <attr name="menuBackColor" format="color|reference" />
        //Menu的按下背景
        <attr name="menuPressedBackColor" format="color|reference" />
        //Menu的按下的字体颜色
        <attr name="menuPressedTitleTextColor" format="color|reference" />
        //列表的背景
        <attr name="menuListBackColor" format="color|reference" />
        //Menu list 的字体颜色
        <attr name="menuListTextColor" format="color|reference" />
        //Menu list 的字体大小
        <attr name="menuListTextSize" format="dimension" />
        //Menu list 选中图标
        <attr name="menuListItemCheckIcon" format="reference" />
        //Menu 下拉图标
        <attr name="menuDropDownSelectedIcon" format="reference" />
        //Menu 上拉图标
        <attr name="menuDropDownUnSelectedIcon" format="reference" />
        //Menu list divider
        <attr name="menuListDivider" format="boolean" />
        //Menu list check
        <attr name="menuListCheck" format="boolean" />
        //Menu arrow title margin
        <attr name="menuArrowMarginTitle" format="dimension" />

    </declare-styleable>

</resources>